{
    "name": "basc/messenger-ui",
    "description": "Laravel messenger suite UI.",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Tippin",
            "email": "tippindev@gmail.com",
            "homepage": "https://tippindev.com"
        }
    ],
    "homepage": "https://github.com/rtippin/messenger-ui",
    "keywords": [
        "Laravel",
        "Messenger"
    ],
    "require": {
        "rtippin/messenger": "^1.19"
    },
    "autoload": {
        "psr-4": {
